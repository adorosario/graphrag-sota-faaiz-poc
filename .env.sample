# =====================
# .env.sample
# =====================
# Copy this file to .env and fill in your actual values

# Neo4j Database Configuration
NEO4J_URI=neo4j+s://your-instance.databases.neo4j.io
NEO4J_USERNAME=neo4j
NEO4J_PASSWORD=your-secure-password

# OpenAI API Configuration (Optimized for GraphRAG latency/cost)
OPENAI_API_KEY=sk-your-openai-api-key-here
OPENAI_EMBEDDING_MODEL=text-embedding-3-small
OPENAI_CHAT_MODEL=gpt-5-nano

# Demo Application Credentials
DEMO_USERNAME_1=admin
DEMO_PASSWORD_1=your-admin-password

# Alternative Model Options (Higher cost/latency but better quality)
# OPENAI_EMBEDDING_MODEL=text-embedding-3-large  # Higher quality embeddings
# OPENAI_CHAT_MODEL=gpt-5-mini                   # Better for complex entity extraction
# OPENAI_CHAT_MODEL=gpt-5                        # Best quality but highest cost